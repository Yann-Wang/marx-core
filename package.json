{
  "name": "marx-core",
  "version": "0.1.3",
  "description": "我是马克思，你们的精神领袖 -- 基于koa2的node框架",
  "scripts": {
    "build:mkdir": "mkdir base filter middleware || true",
    "build:compiler": "ls src | sed 's/\\s+/\\n/g' | xargs -t -I {} catullus babel src/{} --out-dir ./{}",
    "build": "npm run build:mkdir && npm run build:compiler",
    "prepublish": "npm run build"
  },
  "repository": "https://github.com/Yann-Wang/marx-core.git",
  "author": "Raphael, spray",
  "license": "ISC",
  "dependencies": {
    "bunyan": "~1.8.5",
    "koa-body": "~1.6.0",
    "koa-convert": "~1.2.0",
    "koa-generic-session": "~1.11.4",
    "koa-redis": "~2.1.3",
    "koa-router": "~7.0.1",
    "koa-static": "~2.0.0",
    "npm-run-all": "~4.0.1",
    "nunjucks": "~3.0.0",
    "ua-parser-js": "~0.7.13"
  },
  "devDependencies": {
    "catullus": "^1.0.1",
    "babel-eslint": "~7.1.1",
    "eslint": "~3.15.0",
    "eslint-config-airbnb": "~14.1.0",
    "eslint-plugin-import": "~2.2.0",
    "eslint-plugin-jsx-a11y": "~4.0.0",
    "eslint-plugin-react": "~6.9.0"
  }
}
